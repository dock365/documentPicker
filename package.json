  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dock365/documentPicker/issues"
  },
  "homepage": "https://github.com/dock365/documentPicker#readme",
  "devDependencies": {
    "@pnp/common": "^1.3.5",
    "@pnp/logging": "^1.3.5",
    "@pnp/odata": "^1.3.5",
    "@pnp/sp": "^1.3.5",
    "@types/es6-promise": "^3.3.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
